{
    "name": "n_german",
    "vars": {
        "Cache_State": "nat=>nat",
        "Cache_Data": "nat=>nat",
        "Chan1_Cmd": "nat=>nat",
        "Chan1_Data": "nat=>nat",
        "Chan2_Cmd": "nat=>nat",
        "Chan2_Data": "nat=>nat",
        "Chan3_Cmd": "nat=>nat",
        "Chan3_Data": "nat=>nat",
        "InvSet": "boolean",
        "ShrSet": "boolean",
        "ExGntd": "boolean",
        "CurCmd": "nat=>nat",
        "CurPtr": "NODE",
        "MemData": "DATA",
        "AuxData": "DATA"
    },
    "states": [
        "I",
        "S",
        "E",
        "Empty",
        "ReqS",
        "ReqE",
        "Inv",
        "InvAck",
        "GntS",
        "GntE"
    ],
    "init": {
        "var": "i ",
        "guard": "Chan1[i].Cmd = Empty Chan2[i].Cmd = Empty Chan3[i].Cmd = Empty\n    Cache[i].State = I InvSet[i] = false ShrSet[i] = false & ExGntd = false"
    },
    "rules": [],
    "invs": {
        "vars": [
            "i",
            "j"
        ],
        "prop": "~ (i = j) -> Cache[i].State = E -> Cache[j].State = I & Cache[i].State = S -> Cache[j].State = I | Cache[j].State = S"
    }
}